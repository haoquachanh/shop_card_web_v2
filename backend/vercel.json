{
   "version": 2,
   "builds": [
     {
       "src": "index.ts",
       "use": "@vercel/node"
     }
   ],
   "routes": [
     {
       "src": "/api/(.*)",
       "dest": "/$1",
       "methods": ["GET", "POST", "PUT", "DELETE"]
     },
     {
       "src": "/(.*)",
       "dest": "/index.ts"
     }
   ]
 }
 